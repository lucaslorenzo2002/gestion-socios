{
  "name": "sistema-club-rugby",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development  tsc-watch --onSuccess \"node dist/server.js\"",
    "test": "cross-env NODE_ENV=test jest --watchAll --no-cache",
    "seed:init": "ts-node ./src/seeds/init.ts"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/tests/setup.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "collectCoverageFrom": ["src/**/*.{ts,tsx,js,jsx}', '!src/**/*.d.ts"],
    "extensionsToTreatAsEsm": [".ts"],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "isolatedModules": true,
          "useESM": true
        }
      ]
    } ,
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },  

  "keywords": [],
  "author": "",
  "license": "ISC",
